<?xml version="1.0" encoding="utf-8"?>
<selector android:dither="false"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/corner_radius" />
            <stroke android:width="1.0dip" android:color="@color/gray_stroke" />
            <solid android:color="@color/transparent_pressed" />
            <padding android:left="1.0dip" android:top="1.0dip" android:right="1.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/corner_radius" />
            <stroke android:width="1.0dip" android:color="@color/gray_stroke" />
            <solid android:color="@color/transparent" />
            <padding android:left="1.0dip" android:top="1.0dip" android:right="1.0dip" />
        </shape>
    </item>
</selector>